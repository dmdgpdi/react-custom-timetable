{
  "name": "react-custom-timetable",
  "description": "custom timetable for react",
  "version": "0.0.9",
  "keywords": [
    "timetable",
    "scheduler"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dmdgpdi/react-custom-timetable.git"
  },
  "contributors": [
    "dmdgpdi",
    "haejinyun"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/lib/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "yarn clean && yarn build:tsc && yarn build:js",
    "build:tsc": "yarn tsc",
    "build:js": "node esbuild.config.js",
    "clean": "rimraf -rf dist",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.22",
    "date-fns": "^3.6.0",
    "randomcolor": "^0.6.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-inject": "^5.0.5",
    "@types/date-fns": "^2.6.0",
    "@types/randomcolor": "^0.5.9",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.24.0",
    "esbuild-sass-plugin": "^3.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-postprocess": "^1.0.2",
    "rollup-plugin-scss": "^3.0.0",
    "sass": "^1.77.8",
    "typescript": "^5.5.4"
  },
  "license": "MIT",
  "packageManager": "yarn@4.4.1"
}
